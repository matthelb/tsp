installdirs:
	for dir in "$(DESTDIR)$(doc_imagesdir)" "$(DESTDIR)$(html_tutorialdir)" "$(DESTDIR)$(libdocdir)" "$(DESTDIR)$(referencedir)"; do \
	  test -z "$$dir" || $(MKDIR_P) "$$dir"; \
	done
